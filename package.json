{
    "name": "webpack-markup-bootstrap",
    "version": "1.0.0",
    "description": "Webpack markup bootstrap",
    "main": "src/index.js",
    "scripts": {
        "dev": "cross-env  NODE_ENV=development ./node_modules/.bin/webpack-dev-server --progress --watch --inline --mode development",
        "prod": "cross-env  NODE_ENV=production ./node_modules/.bin/webpack-dev-server --progress --watch --inline --mode production",
        "build": "scross-env et NODE_ENV=production del-cli public && webpack --mode production",
        "clean": "del-cli public"
    },
    "devDependencies": {
        "@babel/core": "^7.1.2",
        "@babel/preset-env": "^7.1.0",
        "autoprefixer": "^9.1.5",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.4",
        "copy-webpack-plugin": "^4.5.2",
        "cross-env": "^5.2.0",
        "css-loader": "^1.0.0",
        "cssnano": "^4.1.4",
        "del-cli": "^1.1.0",
        "eslint": "^5.6.1",
        "eslint-loader": "^2.1.1",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.4.3",
        "node-sass": "^4.9.3",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "postcss-loader": "^3.0.0",
        "raw-loader": "^0.5.1",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "uglifyjs-webpack-plugin": "^2.0.1",
        "webpack": "^4.20.2",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.9"
    },
    "dependencies": {
        "normalize.css": "^8.0.0"
    }
}
